{
  "mimeTypes": {
    ".js": "application/javascript",
    ".mjs": "application/javascript",
    ".css": "text/css",
    ".json": "application/json",
    ".html": "text/html",
    ".ico": "image/x-icon",
    ".png": "image/png",
    ".svg": "image/svg+xml",
    ".woff": "font/woff",
    ".woff2": "font/woff2"
  },
  "routes": [
    {
      "route": "/assets/*.js",
      "headers": {
        "Content-Type": "application/javascript"
      }
    },
    {
      "route": "/assets/*.mjs",
      "headers": {
        "Content-Type": "application/javascript"
      }
    },
    {
      "route": "/assets/*.css",
      "headers": {
        "Content-Type": "text/css"
      }
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/assets/*", "/*.js", "/*.mjs", "/*.css", "/*.ico", "/*.png", "/*.svg", "/*.woff", "/*.woff2"]
  }
}
